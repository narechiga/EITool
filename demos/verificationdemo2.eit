
/* requires refinement by parts */
/* SSA0 */

\statevariables{
	dsv
}

\eiparameters{
	vmax, dsvinit
}

\envelope{
	((vmax*2 + dsv <= 0) & (u <= vmax)) | (u <= 0)
}

\invariant{
	dsvinit <= dsv & dsv <= 0
}

\robustparameters{
	(0.1 <= vmax) & (vmax <= 10) & (dsvinit >= -20) & (dsvinit <= -0.1)
}

\domain{
	-20 <= dsv & dsv <= 0
}

\controllaw{
	u = -0.5*(dsv + 0.1)
}

