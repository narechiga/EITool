
/* requires refinement by parts */
/* SSA0 */

\statevariables{
	x
}

\eiparameters{
	start, b, m	
}

\envelope{
	u <= m*(x - b)
}

\invariant{
	start <= x & x <= b
}

\robustparameters{
	/* so it can't be parallel to the control law, must be steeper */
	-1 <= m & m <= 0 & 100 > b & b > 0 & -20 < start & start < 0

}

\domain{
	-20 <= x & x <= 0
}

\controllaw{
	u = -0.5*(x + 0.1)
}

