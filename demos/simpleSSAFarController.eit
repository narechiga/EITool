
/* Far controller */

\statevariables{
	d
}

\initialset{
	/*-21 <= d & d <= -19*/
	-20 <= d & d <= -19
}

\safeset{
	-30 <= d & d <= 0
}

\eiparameterset{
	/* witness at e = 22 */
	(e >= 0 ) & ( e <= 1000 )
	
}

\envelope{
	((0.11*e + d <= 0) & ( u <= e )) | ( u <= 0 )
	
}

\invariant{
	-21<= d & d <= 0
}

\controllaw{
	u := -1*(d + 2.5)
}





