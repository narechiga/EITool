
/* requires refinement by parts */
/* SSA0 */

\statevariables{
	x
}

\eiparameters{
	start, a, b
}

\envelope{
	u <= 1/(x+a) + b
}

\invariant{
	start <= x & 1/(x-a) + b >= 0
}

\robustparameters{
	/*-20 <= start & b <= -0.2 & b - start >= 1*/

	-20 <= start & start <= -1/a + b & a < 0 & b > 0 & b < 10

}

\domain{
	-20 <= x & x <= 0.1
}

\controllaw{
	u := -0.5*(x + 0.1)
}

